#include<iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int year,month,day;
        cin>>year>>month>>day;
        if(month==9&&day==30)
            cout<<"YES"<<endl;
        else if(month==11&&day==30)
            cout<<"YES"<<endl;
        else if((month+day)%2==0)
            cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
}
